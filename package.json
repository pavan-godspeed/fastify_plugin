{
	"name": "fastify_plugin",
	"version": "1.0.0",
	"description": "My first godspeed project.",
	"main": "dist/index.js",
	"scripts": {
		"dev": "nodemon -r dotenv ./src/index.ts",
		"gen-crud-api": "npx @godspeedsystems/api-generator",
		"build": "NODE_ENV=production swc src -d dist && rsync -a --exclude '*.ts' ./src/ ./dist/",
		"preview": "node dist/index.js",
		"serve": "nodemon --watch src -e ts,js,yaml,json --exec 'npm run build && npm run preview'",
		"clean": "rm -rf dist"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@fastify/middie": "^8.3.0",
		"@godspeedsystems/tracing": "^0.0.7",
		"@mindgrep/express-prometheus-middleware": "^0.0.3",
		"@swc/cli": "^0.1.62",
		"@swc/core": "^1.3.93",
		"@types/config": "^3.3.0",
		"@types/fastify-jwt": "^0.8.1",
		"@types/lodash": "^4.14.201",
		"@types/node": "^18.18.9",
		"nodemon": "^2.0.20",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.5"
	},
	"dependencies": {
		"@fastify/cors": "^8.4.1",
		"@fastify/express": "^2.3.0",
		"@fastify/formbody": "^7.4.0",
		"@fastify/jwt": "^7.2.3",
		"@fastify/swagger": "^8.12.0",
		"@fastify/swagger-ui": "^1.10.1",
		"@godspeedsystems/api-generator": "latest",
		"@godspeedsystems/core": "latest",
		"@godspeedsystems/plugins-axios-as-datasource": "latest",
		"@godspeedsystems/plugins-express-as-http": "latest",
		"@godspeedsystems/plugins-prisma-as-datastore": "latest",
		"@prisma/client": "^5.2.0",
		"config": "^3.3.9",
		"dotenv": "^16.0.3",
		"fastify": "^4.24.3",
		"fastify-oas": "^3.0.8",
		"fastify-swagger": "^5.2.0",
		"fastify-swagger-ui": "^1.0.1",
		"pino-pretty": "^10.2.0",
		"prisma": "^5.2.0",
		"qs": "^6.11.2"
	}
}
